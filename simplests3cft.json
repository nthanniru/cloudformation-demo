{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": "i-named-this-bucket1991"
              }
        }
    },
    "MyEC2Instance" : {
        "Type" : "AWS::EC2::Instance",
        "Properties" : {
           "ImageId" : "ami-0022f774911c1d690",
           "KeyName" : "devops"
     },
    "Outputs": {
        "BucketName": {
            "Value": {
                "Ref": "S3Bucket"
            },
            "Description": "Name of the sample Amazon S3 bucket."
        },
        "InstanceName": {
            "Value": {
                "Ref": "MyEC2Instance"
            },
            "Description": "Name of the sample Amazon EC2 Machine."
        }
    }
}
}
